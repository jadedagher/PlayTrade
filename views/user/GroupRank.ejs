<div class="plafond"></div><!-- _________________________ranking Panel_________________________ --><div class="container">    <div class="row">        <div class="GroupRanking">            <h3>Group Ranking</h3>            <table class = 'table'>                <tr>                    <th>Team Name</th>                    <th>Team Wallet</th>                </tr>                <%_.each(GroupRanking, function(GroupRanking){	%>                <tr data-id="<% GroupRanking.id %>" data-model="UserRanking">                    <td><%= GroupRanking.GroupName%></td>                    <td><%= GroupRanking.GroupWallet +' $'%></td>                </tr>                <% }) %>            </table>            <hr>        </div>    </div></div>